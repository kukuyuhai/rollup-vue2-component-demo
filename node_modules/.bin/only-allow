#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules/.pnpm/registry.npmmirror.com+only-allow@1.0.0/node_modules/only-allow/node_modules:/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules/.pnpm/registry.npmmirror.com+only-allow@1.0.0/node_modules:/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules/.pnpm/node_modules:/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules:/Users/chencong/workspace/DataExchange/node_modules:/Users/chencong/workspace/node_modules:/Users/chencong/node_modules:/Users/node_modules:/node_modules:/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules/only-allow/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules/.pnpm/registry.npmmirror.com+only-allow@1.0.0/node_modules/only-allow/node_modules:/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules/.pnpm/registry.npmmirror.com+only-allow@1.0.0/node_modules:/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules/.pnpm/node_modules:/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules:/Users/chencong/workspace/DataExchange/node_modules:/Users/chencong/workspace/node_modules:/Users/chencong/node_modules:/Users/node_modules:/node_modules:/Users/chencong/workspace/DataExchange/chinadepLogin/node_modules/only-allow/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../only-allow/bin.js" "$@"
else
  exec node  "$basedir/../only-allow/bin.js" "$@"
fi
